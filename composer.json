{
    "name": "xola/elasticsearch-proxy-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony2 plugin that acts as an authorization proxy for elasticsearch",
    "keywords": ["elasticsearch", "symfony", "search", "proxy", "authorization"],
    "homepage": "https://github.com/xola/XolaElasticsearchProxyBundle",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "symfony/framework-bundle": ">=4",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "Xola\\ElasticsearchProxyBundle\\": ""
        }
    }
}
